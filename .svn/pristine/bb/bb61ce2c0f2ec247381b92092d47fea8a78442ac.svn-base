	font-family: "Helvetica Narrow","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
	font-style:12px;
	font-weight:normal;
	padding:5px;
	margin-top: 6px;
}
.custinfocrttbl input[type="button"] select {
    background-color: #5F8694;
    border: 0 none;
    color: #FFFFFF;
    font-family: "Helvetica Narrow","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
    font-size: 17px;
    height: 35px;
    padding: 4px;
    width: 85px;
}
/*create/edit starts here*/
.custinfocrttbla {
	color: #878F3F;
	/*display: none;*/
    float: left;
	font: normal 12px "Arial Narrow";
