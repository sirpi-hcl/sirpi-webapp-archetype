    font: 17px segoe ui !important;
    height: 35px;
    padding: 4px;
    width: 85px;
}
table.gridtable {
	font: normal 12px arial !important;
	color:#333333;
	border-width: 1px;
	border-color: #7da289;
	border-collapse: collapse;
}
table.gridtable th {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #7da289;
	background-color: #d2eec9;
}
table.gridtable td {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #7da289;
	background-color: #ffffff;
}
#litbx {
	display: none;
	height: 100%;
	left: 0;
	position: absolute;
	top: 0;
	width: 100%;
	background-image:url("#{resource['img:lit_box.png']}");
	background-repeat:repeat;
}
.litbx {
	float:left;
