/*create/edit starts here*/
.custinfocrttbl {
	background-color: #FFFFFF;
	border: 1px solid #CCCCCC;
	color: #5F8694;
	/*display: none;*/
    float: left;
	font: normal 14px Arial;
	left: 373px;
	margin: 0 auto;
	padding: 20px;
